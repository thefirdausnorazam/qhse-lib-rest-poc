	<parameter name="dynamicSelect" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{sqlVersion}.equals("SQLServer2008") ?

" select cast(dateadd(dd,-(day(start_time)-1),start_time) as date) as month, dbo.substring_delim(isnull(dept.name, 'Third Party'), '$P!{departmentDelim}') as department, sum(q.score)*100/sum(q.score_max) as pct"
+ " from audit a"
+ " inner join audit_question q on a.id = q.audit"
+ " inner join auditee ae on a.auditee = ae.id"
+ " left join common_questions_option dept on ae.department = dept.id"
+ " where a.status in ('IN_PROGR','COMPLETE') AND a.programme = $P!{auditProgrammeId} and q.score is not null"
+ " group by dbo.substring_delim(isnull(dept.name, 'Third Party'), '$P!{departmentDelim}'), cast(dateadd(dd,-(day(start_time)-1),start_time) as date)"
+ " order by department, month"
:
" select date(subdate(start_time,dayofmonth(start_time)-1)) month, substring_index(ifnull(dept.name, 'Third Party'), '$P!{departmentDelim}', 1) as department, sum(q.score)*100/sum(q.score_max) as pct"
+ " from audit a"
+ " inner join audit_question q on a.id = q.audit"
+ " inner join auditee ae on a.auditee = ae.id"
+ " left join common_questions_option dept on ae.department = dept.id"
+ " where a.status in ('IN_PROGR','COMPLETE') AND a.programme = $P!{auditProgrammeId} and q.score is not null"
+ " group by substring_index(ifnull(dept.name, 'Third Party'), '$P!{departmentDelim}', 1), date(subdate(start_time,dayofmonth(start_time)-1))"
		]]></defaultValueExpression>
