"LEFT JOIN action_holder h ON i.action_holder_id = h.id " +
"LEFT JOIN action a ON h.current_action_id = a.id " +
"WHERE i.active = 1 AND c.name IS NOT null AND date(occurred_time) BETWEEN '$P!{startofYear}' AND '$P!{endofYear}' " +
"ORDER BY cause, occurred_time"
]]></defaultValueExpression>
