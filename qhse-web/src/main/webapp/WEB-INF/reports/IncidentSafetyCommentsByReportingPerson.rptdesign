            </list-property>
            <xml-property name="queryText"><![CDATA[select 
  u.user_name, u.first_name, u.last_name, count(t.id) as safetyCommentCount
from incident i
inner join sec_user u on i.created_by=u.id
left join common_questions_option dept on i.department = dept.id
inner join incident_type st on i.subtype_id = st.id
inner join incident_type t on st.parent_id = t.id
where t.name = 'Safety_Comment'
and i.active=1
and i.occurred_time between ? and ?
group by u.user_name, u.first_name, u.last_name,i.created_by
order by i.created_by]]></xml-property>
