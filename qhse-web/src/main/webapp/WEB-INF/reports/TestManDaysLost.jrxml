	<parameter name="startYear" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["2005"]]></defaultValueExpression>
	</parameter>
	<parameter name="endYear" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA["2006"]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT dept.name dept, months.id mth, sum(man_days_lost) man_days_lost 
FROM common_qanda_questions_question_assoc_option qao
left join common_qanda_questions_option dept on dept.id = qao.oid
, integers months
left join incident inc on month(occurred_time) = months.id 
and dept.id = inc.department and inc.active = 1 
and EXTRACT(year FROM occurred_time)between $P{startYear} and $P{endYear} 
left join investigation inv on inv.id = inc.id
where qid = 42 and visible = 1 
and months.id between 1 and 12
group by dept,mth]]></queryString>

	<field name="dept" class="java.lang.String"/>
	<field name="mth" class="java.lang.Long"/>
	<field name="man_days_lost" class="java.lang.Long"/>

	<variable name="totalDaysLost" class="java.lang.Long" resetType="Report" calculation="Sum">
		<variableExpression><![CDATA[$F{man_days_lost}]]></variableExpression>
	</variable>
	<variable name="daysLost" class="java.lang.Long" resetType="None" calculation="Nothing">
		<variableExpression><![CDATA[$F{man_days_lost} == null ? new Long(0) : $F{man_days_lost}]]></variableExpression>
	</variable>
	<variable name="totalDeptDaysLost" class="java.lang.Long" resetType="Group" resetGroup="department" calculation="Sum">
		<variableExpression><![CDATA[$V{daysLost}]]></variableExpression>
	</variable>
	<variable name="days_lost" class="java.lang.Long" resetType="Group" resetGroup="department" calculation="Nothing">
